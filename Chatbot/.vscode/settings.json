{
    "Lua.runtime.version": "Lua 5.1",
    "Lua.runtime.builtin": {
        "basic": "disable",
        "debug": "disable",
        "io": "disable",
        "math": "disable",
        "os": "disable",
        "package": "disable",
        "string": "disable",
        "table": "disable",
        "utf8": "disable"
    },
    "Lua.workspace.library": [
        "${workspaceFolder}]\\Libs",
        "~\\.vscode\\extensions\\ketho.wow-api-0.21.0\\Annotations\\Core"
    ],
    "Lua.diagnostics.globals": [
        "SELECTED_CHAT_FRAME",
        "DEFAULT_CHAT_FRAME",
        "ACCEPT",
        "CANCEL",
        "SlashCmdList",
        "hash_SlashCmdList",
        "NORMAL_FONT_COLOR_CODE",
        "FONT_COLOR_CODE_CLOSE",
        "LE_PARTY_CATEGORY_INSTANCE",
        "LE_PARTY_CATEGORY_HOME",
        "UIParent",
        "NORMAL_FONT_COLOR",
        "GameFontNormal",
        "GameFontHighlight",
        "GameFontHighlightLarge",
        "GameFontHighlightSmall"
    ],
    "cSpell.allowCompoundWords": true,
    "cSpell.words": [
        "draenei",
        "Dwarven",
        "Orcish",
        "Taur-ahe",
        "Thalassian",
        "Zandali"
    ],
    "cSpell.ignoreRegExpList": ["\\|c[A-Fa-f0-9]{6,8}"]
}
